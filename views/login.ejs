<%- include('./partials/header') %>
<form id="signin-form" class="form-signin">
    <div class="text-center mb-4">
        <img class="mb-4" src="/img/logo.jpg" alt="" width="72" height="72">
    </div>

    <div class="form-label-group">
        <input type="email" id="email" class="form-control" placeholder="Email address" required autofocus>
        <label for="email">Email address</label>
    </div>

    <div class="form-label-group">
        <input type="password" id="password" class="form-control" placeholder="Password" required>
        <label for="password">Password</label>
    </div>

    <button class="btn btn-lg btn-primary btn-block" type="submit" name="signinbtn" id="signinbtn" value="signinbtn">Sign in</button>

    <%- include('./partials/passwordresetandforgetpasswordblock')%>

</form>
<%- include('./partials/footer') %>
